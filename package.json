{
  "name": "heritage-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "test:all": "npm test -- --run && npm run e2e",
    "server:start": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:logs": "docker logs heritage_tracker_db -f",
    "db:migrate": "node database/scripts/migrate.js",
    "db:migrate:status": "node database/scripts/migration-status.js",
    "db:extract-json": "node database/scripts/extract-mock-data.js",
    "db:generate-seed": "node database/scripts/generate-seed.js",
    "db:seed": "node database/scripts/seed.js",
    "db:setup": "npm run db:start && timeout /t 5 && npm run db:migrate && npm run db:extract-json && npm run db:generate-seed && npm run db:seed",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\" --names \"vite,server\" --prefix-colors \"cyan,green\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/leaflet.heat": "^0.2.5",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "d3-scale": "^4.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.5.3",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.9.4",
    "react-window": "^2.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/d3-scale": "^4.0.9",
    "@types/leaflet": "^1.9.20",
    "@types/leaflet.markercluster": "^1.5.6",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.6",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
